    "n, m  = input().split(' ')\n",
    "x = list(map(int, input().split()))\n",
    "a = set(map(int, input().split()))\n",
    "b = set(map(int, input().split()))\n",
    "sum = 0\n",
    "for value in x:\n",
    "    if value in a: \n",
    "        sum += 1\n",
    "    elif value in b: \n",
    "        sum += -1\n",
    "    else:\n",
    "        sum += 0\n",
    "print(sum)"
